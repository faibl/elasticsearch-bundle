    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Kandulla",
            "email": "hannes@faibl.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "elasticsearch/elasticsearch": "^7.16"
